<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MoM.ClubSub.TicketPage">
    <ContentPage.Content>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="AUTO"/>
            </Grid.RowDefinitions>
            <BoxView HeightRequest="200" WidthRequest="200" Grid.Column="0" Grid.Row="0"/>
            <StackLayout BackgroundColor="grey" Grid.Column="1" Grid.Row="0" Grid.ColumnSpan="1" >
                <Label Text="You have voted on"/>
                <Label Text="Nicolai Simonsen"/>
                <Label Text="Team 1 Vs Team 2"/>
                <Label Text="16/5/2018 - Kl: 10:00"/>
            </StackLayout>
        </Grid>
    </ContentPage.Content>
</ContentPage>